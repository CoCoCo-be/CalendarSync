<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="be.CoCoCo.CalendarSync.Mapping" table="mapping">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="identity" />
        </id>
        <property name="UID1" type="string">
            <column name="UID1" length="255" not-null="true" unique="true" />
        </property>
        <property name="UID2" type="string">
            <column name="UID2" length="255" not-null="true" unique="true" />
        </property>
    </class>
</hibernate-mapping>