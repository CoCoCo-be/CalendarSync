<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="be.CoCoCo.CalendarSync.UUID" table="UUID">
        <id name="id" type="java.lang.Integer">
        	<column name="id" />
        	<generator class="assigned" />
        </id>
    </class>

    <query name="getID">
        <![CDATA[select max(id) from UUID]]>
    </query>

    <query name="reset">
        <![CDATA[delete from UUID]]>
    </query>
	
</hibernate-mapping>